{
    "name": "archipro/silverstripe-revolt-event-dispatcher",
    "description": "A Revolt Event Dispatcher integration for Silverstripe CMS",
    "type": "silverstripe-vendormodule",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^4.13 || ^5.0",
        "silverstripe/versioned": "^1.13 || ^2.0",
        "psr/event-dispatcher": "^1.0",
        "psr/event-dispatcher-implementation": "^1.0",
        "archipro/revolt-event-dispatcher": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "ArchiPro\\Silverstripe\\EventDispatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArchiPro\\Silverstripe\\EventDispatcher\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix --dry-run --diff",
        "lint-fix": "php-cs-fixer fix",
        "analyse": "phpstan analyse",
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "expose": []
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:archiprocode/revolt-event-dispatcher.git"
        }
    ]
}
